.card{
  border-radius: 20rpx;
  background: #fff;
  box-shadow:0px 0px 15px 0px rgba(0,0,0,0.25);
  padding: 40rpx 30rpx;
  margin: -78rpx 62rpx 0;
}
.btn{
  font-size: 30rpx;
  height: 80rpx;
  line-height: 78rpx;
  border-radius: 100rpx;
  border: 1rpx solid;
}
.btn1{
  color: #27AAFF;
  border-color: #27AAFF;
  background: #fff;
  display: inline-block;
}
.btn2{
  color: #fff;
  background: #1890FF;
  border-color: #1890FF;
  box-shadow:0px 2px 10px 0px rgba(183,227,255,1);
}
.fixed-bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.custom-navbar .nav-bar{
  width: 100%;
  height: 44px;
  line-height: 44px;
  background: #1890FF;
  color: #fff;
  /* font-size: 34rpx; */
  text-align: center;
  font-weight: bold;
  position: relative;
}
.custom-navbar image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 10rpx;
  top: 24rpx;

}