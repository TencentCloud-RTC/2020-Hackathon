.flex{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.title{
    color: #333333;
    font-size: 28rpx;
    border-bottom: 1px solid #DDDDDD;
    padding: 26rpx 30rpx;
}
.title image{
    width: 44rpx;
    height: 44rpx;
}
.list{
    padding: 60rpx 30rpx;
}
.list .item{
    background: #fff;
    box-shadow:0rpx 5rpx 30rpx 0rpx rgba(0,0,0,0.12);
    border-radius:16rpx;
    margin-bottom: 30rpx;
    padding: 30rpx 0;
}
.item .status{
    width: 142rpx;
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 8rpx;
    font-size: 24rpx;
    color: #fff;
    margin-bottom: 28rpx;
    margin-left: 49rpx;
}
.status-waiting{
    background: #FF9518;
}
.status-going{
    background: #1890FF;
}
.status-complete{
    background: #4CBD66;
}
.item .status b{
    width: 16rpx;
    height: 16rpx;
    background: #fff;
    border-radius: 16rpx;
    display: inline-block;
    margin-left: 20rpx;
    margin-right: 10rpx;
}
.clock{
    margin: 0 49rpx 39rpx;
    display: flex;
}
.clock b{
    color: #333333;
    font-size: 48rpx;
}
.clock .bg{
    width: 240rpx;
    position: relative;
    color: #1890FF;
    font-size: 36rpx;
    text-align: center;
    margin: 0 59rpx;
    line-height: 64rpx;
}
.clock .bg .icon{
    width: 240rpx;
    height: 12rpx;
    position: absolute;
    left: 0;
    top: 28rpx;
}
.line{
    width: 100%;
    border: 1px dashed #F2F2F2;
    position: relative;
    margin-bottom: 30rpx;
}
.line image{
    width: 30rpx;
    height: 60rpx;
    position: absolute;
    right: 0;
    top: -30rpx;
}
.text{
    font-size: 28rpx;
    color: #888888;
}
.text .confirm{
    color: #333;
}
.mr-20{
    margin-right: 20rpx;
}
.mrl-36{
    margin: 0 36rpx;
}
.mb-20{
    margin-bottom: 20rpx;
}
.inline{
    display: flex;
}
.non{
    color: #888;
}
.non image{
    width: 25rpx;
    height: 25rpx;
    margin-right: 6rpx;
}
.no-data{
    color: #999;
    text-align: center;
    font-size: 24rpx;
    padding: 30rpx;
}

/* 日历 */
.cale{
    background: #fff;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1000;
    padding: 30rpx;
}